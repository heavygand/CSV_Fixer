<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="CSV.Fixer.plugin.commands.category"
            id="CSV.Fixer.plugin.commands.FixTGCounterCommand"
            name="Fix telegramcounter in selected editor">
      </command>
      <command
            categoryId="CSV.Fixer.plugin.commands.category"
            id="CSV.Fixer.plugin.commands.toggleCommentCommand"
            name="Toggle comment in selected line">
      </command>
      <command
            defaultHandler="csv.fixer.plugin.handlers.KontextBefehlHandler"
            id="CSV.Fixer.plugin.commands.kontextBefehl"
            name="Timestamp(s) entfernen">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="CSV.Fixer.plugin.commands.FixTGCounterCommand"
            class="csv.fixer.plugin.handlers.CSVFixerHandler">
      </handler>
      <handler
            class="csv.fixer.plugin.handlers.CSVToggleCommentHandler"
            commandId="CSV.Fixer.plugin.commands.toggleCommentCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="CSV.Fixer.plugin.commands.FixTGCounterCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
      <key
            commandId="CSV.Fixer.plugin.commands.toggleCommentCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+7">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="CSV"
               mnemonic="M"
               id="CSV.Fixer.plugin.menus.csvFixer">
            <command
                  commandId="CSV.Fixer.plugin.commands.FixTGCounterCommand"
                  mnemonic="S"
                  id="CSV.Fixer.plugin.menus.FixTGCounterCommand">
            </command>
            <command
                  commandId="CSV.Fixer.plugin.commands.toggleCommentCommand"
                  id="CSV.Fixer.plugin.menus.toggleCommentCommand"
                  mnemonic="S">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="CSV.Renamer.plugin.menuContribution3">
      </menuContribution>
      <menuContribution
            locationURI="popup:#TextEditorContext?after=additions">
         <command
               commandId="CSV.Fixer.plugin.commands.kontextBefehl"
               icon="icons/myC.png">
         </command>
      </menuContribution>
   </extension>
</plugin>
